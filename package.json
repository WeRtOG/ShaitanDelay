{
    "name": "shaitan-delay",
    "version": "0.1.0",
    "homepage": ".",
    "private": true,
    "dependencies": {
        "@elemaudio/plugin-renderer": "^1.0.18",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^29.2.5",
        "@types/node": "^18.11.18",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "archiver": "^5.3.1",
        "axios": "^1.2.2",
        "bootstrap-icons": "^1.10.3",
        "js-md5": "^0.7.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "sass": "^1.57.1",
        "typescript": "^4.9.4",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "concurrently -m 1 -p none \"npm run build-ui\" \"npm run build-vst\" \"npm run make-zip\"",
        "build-ui": "react-scripts build",
        "build-vst": "concurrently -m 1 -p none \"node ./scripts/preBuild.js\" \"npx elem-export --outputDir=dist --manifest=build/elementary.config.json build/\"",
        "publish": "node ./scripts/publish.js",
        "make-zip": "node ./scripts/makeZIP.js",
        "make-ssl": "bash ./dev/generateSSL.sh",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "win-install": "cd dev && installDist.bat",
        "win-uninstall": "cd dev && uninstallDist.bat",
        "win-update": "cd dev && reinstallDist.bat",
        "win-build-update": "concurrently -m 1 -p none \"npm run build\" \"npm run win-update\""
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^7.6.0",
        "prettier": "2.8.1"
    }
}
